@model IEnumerable<ProyectoFinal.Models.Nominas>

@{
    ViewBag.Title = "Listar";
}

<h2>Nominas</h2>

<p>
    @Html.ActionLink("Create New", "Index")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Año)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Mes)
        </th>
        <th>
            Total de Nomina
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Año)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Mes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Total_Nomina)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID_Nominas }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID_Nominas }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID_Nominas })
        </td>
    </tr>
}

</table>
